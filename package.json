{
  "main": "src/index.ts",
  "scripts": {
    "server:dev": "tsx watch src/index.ts",
    "server:start": "node dist/index.js",
    "server:build.watch": "tsc --watch",
    "server:build": "tsc",
    "server:inspect": "npm run server:build && npx @modelcontextprotocol/inspector dist/index.js",
    "server:inspect:dev": "npm run server:build && set DANGEROUSLY_OMIT_AUTH=true && npx @modelcontextprotocol/inspector dist/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/inspector": "^0.16.1",
    "@modelcontextprotocol/sdk": "^1.16.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
